{
  "name": "@curatedotfun/notion",
  "version": "0.0.6",
  "description": "Notion plugin for curatedotfun",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/potlock/curatedotfun-plugins.git",
    "directory": "packages/notion"
  },
  "keywords": [
    "curatedotfun",
    "notion",
    "distribute",
    "plugin"
  ],
  "scripts": {
    "build": "rspack build && tsc -p tsconfig.build.json",
    "dev": "rspack serve",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.15"
  },
  "devDependencies": {
    "@curatedotfun/types": "workspace:*",
    "@curatedotfun/utils": "workspace:*",
    "@rspack/cli": "latest",
    "typescript": "^5.0.0"
  }
}
